<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.fms.core.entity">
	<class name="ParameterSet" table="PARAMETERSET">
		<id name="id" column="ID">
			<generator class="uuid" />
		</id>
		<property name="createDate" column="CREATEDATE" />
		<property name="modifyDate" column="MODIFYDATE" />


		<property name="qtyBits" column="QTYBITS" />
		<property name="weightBits" column="WEIGHTBITS" />
		<property name="priceBits" column="PRICEBITS" />
		<property name="amountBits" column="AMOUNTBITS" />
		<property name="printCount" column="PRINTCOUNT" />
	</class>
</hibernate-mapping>
