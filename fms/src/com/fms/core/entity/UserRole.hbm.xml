<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.fms.core.entity">
	<class name="UserRole" table="USER_ROLE">
		<id name="id" column="ID">
			<generator class="uuid" />
		</id>
		<property name="createDate" column="CREATEDATE" />
		<property name="modifyDate" column="MODIFYDATE" />
		<many-to-one name="user" class="AclUser" cascade="save-update"
			fetch="select">
			<column name="id"></column>
		</many-to-one>
		<many-to-one name="role" class="Role" cascade="save-update"
			fetch="select">
			<column name="id"></column>
		</many-to-one>
	</class>
</hibernate-mapping>